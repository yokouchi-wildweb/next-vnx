{
  "domainConfigVersion": "1.2",
  "singular": "save",
  "plural": "saves",
  "label": "セーブデータ",
  "dbEngine": "Neon",
  "idType": "uuid",
  "relations": [
    {
      "domain": "user",
      "label": "ユーザー",
      "fieldName": "user_id",
      "fieldType": "uuid",
      "relationType": "belongsTo",
      "required": true,
      "onDelete": "CASCADE"
    }
  ],
  "useCreatedAt": true,
  "useUpdatedAt": true,
  "useSoftDelete": false,
  "fields": [
    {
      "name": "scenario_id",
      "label": "シナリオID",
      "fieldType": "string",
      "formInput": "textInput",
      "required": true
    },
    {
      "name": "slot_number",
      "label": "スロット番号",
      "fieldType": "integer",
      "formInput": "numberInput",
      "required": true,
      "defaultValue": 0
    },
    {
      "name": "play_state",
      "label": "プレイ状態",
      "fieldType": "jsonb",
      "formInput": "hidden",
      "required": true
    },
    {
      "name": "thumbnail",
      "label": "サムネイル（base64）",
      "fieldType": "string",
      "formInput": "hidden",
      "required": false
    },
    {
      "name": "play_time",
      "label": "プレイ時間（秒）",
      "fieldType": "integer",
      "formInput": "numberInput",
      "required": false,
      "defaultValue": 0
    }
  ],
  "searchFields": [
    "scenario_id"
  ],
  "defaultOrderBy": [
    ["updatedAt", "DESC"]
  ],
  "tableFields": [
    "scenario_id",
    "slot_number",
    "play_time",
    "updatedAt"
  ],
  "useDetailModal": false,
  "addToAdminDataMenu": false,
  "useDuplicateButton": false,
  "generateFiles": {
    "entities": true,
    "components": false,
    "hooks": true,
    "clientServices": true,
    "serverServices": true,
    "adminRoutes": false,
    "registry": true,
    "fieldConstants": true,
    "presenters": false
  }
}
